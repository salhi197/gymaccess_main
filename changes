            if(json_decode($request['abonnement'])->id==77+2 or json_decode($request['abonnement'])->id==77+5 or json_decode($request['abonnement'])->id==77+8 or json_decode($request['abonnement'])->id==77+5 ){
                $fin =  Carbon::parse($request['debut'])->addDays(7)->format('Y-m-d');
                $inscription->fin=$fin; 
            }
            if(json_decode($request['abonnement'])->id==77+3 or json_decode($request['abonnement'])->id==77+6 or json_decode($request['abonnement'])->id==77+9 or json_decode($request['abonnement'])->id==77+12 ){
                $fin =  Carbon::parse($request['debut'])->addDays(15)->format('Y-m-d');
                $inscription->fin=$fin; 
            }


            if(json_decode($request['abonnement'])->id==77+4 or json_decode($request['abonnement'])->id==77+7 or json_decode($request['abonnement'])->id==77+10 or json_decode($request['abonnement'])->id==77+13 ){
                $fin =  Carbon::parse($request['debut'])->addDays(15)->format('Y-m-d');
                $inscription->fin=$fin; 
            }
